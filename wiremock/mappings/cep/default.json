{
  "request": {
    "method": "GET",
    "urlPath": "/cep",
    "queryParameters": {
      "cep": {
        "matches": "^(?!01001000|01002000|01003000|01004000|01005000|01006000|01007000|01008000|01009000|01010000).*$"
      }
    }
  },
  "response": {
    "status": 404,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "error": "CEP not found",
      "message": "The provided CEP does not exist in the database"
    }
  }
}